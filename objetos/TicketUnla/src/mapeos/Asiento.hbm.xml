<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Asiento" table="asiento">
		<id column="idAuditorio" name="idAuditorio">
			<generator class="identity">
			</generator>
		</id>
		<property column="codigo" name="codigo" type="string"/>
		
		<many-to-one name="sectorNumerado" class="datos.SectorNumerado" column="idSectorNumerado"
		not-null="true"/>
		
	</class>
</hibernate-mapping>